    "lint": "pnpm run --parallel /^.*:check/",
    "lint-fix": "pnpm run /^.*:fix/",
    "clean": "rimraf build",
    "prebuild": "pnpm lint",
    "postgenerate": "pnpm build",
    "generate": "graphql-codegen --config codegen.yml"
