{
  "name": "@next-ts-cloudfront-ssg-with-backend-template/backend",
  "version": "0.1.0",
  "bin": {
    "backend": "bin/backend.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "prettier:check": "prettier --check bin lib lambda",
    "eslint:check": "eslint './{bin,lib,lambda}/**/*.ts'",
    "prettier:fix": "prettier --write bin lib lambda",
    "eslint:fix": "eslint --fix './{bin,lib,lambda}/**/*.ts'",
    "lint": "pnpm run --parallel /^.*:check/",
    "lint-fix": "pnpm run /^.*:fix/",
    "clean": "rimraf build",
    "prebuild": "pnpm lint",
    "postgenerate": "pnpm build",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "devDependencies": {
    "@apollo/server": "^4.10.2",
    "@as-integrations/aws-lambda": "^3.1.0",
    "@aws-lambda-powertools/logger": "^2.0.4",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/jsdoc": "^3.0.0",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-resolvers": "^4.0.6",
    "@graphql-tools/graphql-file-loader": "^8.0.1",
    "@graphql-tools/load": "^8.0.2",
    "@graphql-tools/schema": "^10.0.3",
    "@types/aws-lambda": "^8.10.137",
    "@types/node": "^20.12.7",
    "aws-cdk": "^2.136.1",
    "esbuild": "^0.20.2",
    "graphql-codegen-typescript-validation-schema": "^0.14.1",
    "log4js": "^6.9.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "yup": "^1.4.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.552.0",
    "aws-cdk-lib": "^2.136.1",
    "aws-lambda": "^1.0.7",
    "constructs": "^10.3.0",
    "graphql": "^16.8.1",
    "source-map-support": "^0.5.21"
  },
  "engines": {
    "npm": "use pnpm please!",
    "yarn": "use pnpm please!",
    "pnpm": ">=8.7.4"
  }
}
