    "@aws-sdk/client-s3": "^3.400.0",
    "aws-cdk-lib": "^2.93.0",
    "aws-lambda": "^1.0.7",
    "constructs": "^10.2.69",
    "graphql": "^16.8.0",
    "source-map-support": "^0.5.21"
  },
  "packageManager": "pnpm@8.6.12",
  "engines": {
    "npm": "use pnpm please!",
    "yarn": "use pnpm please!",
    "pnpm": ">=8.6.12"
  }
