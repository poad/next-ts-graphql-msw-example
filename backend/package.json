{
  "name": "@next-ts-cloudfront-ssg-with-backend-template/backend",
  "version": "0.1.0",
  "bin": {
    "backend": "bin/backend.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "format-check": "rome format bin lib lambda",
    "format": "rome format --write bin lib lambda",
    "check": "rome check bin lib lambda",
    "check-fix": "rome check --apply-unsafe bin lib lambda",
    "lint": "pnpm format-check && pnpm check",
    "lint-fix": "pnpm format && pnpm check-fix",
    "clean": "rimraf build",
    "prelint": "pnpm install --no-frozen-lockfile",
    "prelint-fix": "pnpm install --no-frozen-lockfile",
    "prebuild": "pnpm lint",
    "pregenerate": "pnpm install --no-frozen-lockfile",
    "postgenerate": "pnpm build",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "devDependencies": {
    "@apollo/server": "^4.9.1",
    "@as-integrations/aws-lambda": "^3.0.0",
    "@aws-lambda-powertools/logger": "^1.12.1",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/introspection": "^4.0.0",
    "@graphql-codegen/jsdoc": "^2.3.6",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@graphql-eslint/eslint-plugin": "^3.20.1",
    "@graphql-tools/graphql-file-loader": "^8.0.0",
    "@graphql-tools/load": "^8.0.0",
    "@graphql-tools/schema": "^10.0.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.76",
    "@swc/helpers": "^0.5.1",
    "@types/aws-lambda": "^8.10.119",
    "@types/node": "^20.4.9",
    "aws-cdk": "^2.90.0",
    "esbuild": "^0.19.0",
    "graphql": "*",
    "graphql-codegen-typescript-validation-schema": "^0.11.1",
    "log4js": "^6.9.1",
    "regenerator-runtime": "^0.14.0",
    "rome": "^12.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "yup": "^1.2.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.388.0",
    "aws-cdk-lib": "^2.90.0",
    "aws-lambda": "^1.0.7",
    "constructs": "^10.2.69",
    "graphql": "^16.7.1",
    "source-map-support": "^0.5.21"
  },
  "packageManager": "pnpm@8.6.11",
  "engines": {
    "npm": "use pnpm please!",
    "yarn": "use pnpm please!",
    "pnpm": ">=8.6.11"
  }
}
