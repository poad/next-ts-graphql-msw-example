{
  "name": "@next-ts-cloudfront-ssg-with-backend-template/backend",
  "version": "0.1.0",
  "bin": {
    "backend": "bin/backend.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "eslint-check": "eslint --ext .ts,.js,.tsx,.jsx bin lib lambda",
    "eslint-fix": "eslint --fix --ext .ts,.js,.tsx,.jsx bin lib lambda",
    "prettier-check": "prettier --check bin lib lambda",
    "prettier-format": "prettier --write bin lib lambda",
    "lint": "pnpm prettier-check && pnpm eslint-check",
    "lint-fix": "pnpm prettier-format && pnpm eslint-fix",
    "clean": "rimraf build",
    "prelint": "pnpm install -r",
    "prelint-fix": "pnpm install -r",
    "prebuild": "pnpm lint",
    "pregenerate": "pnpm install -r",
    "postgenerate": "pnpm build",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "devDependencies": {
    "@apollo/server": "^4.9.2",
    "@as-integrations/aws-lambda": "^3.1.0",
    "@aws-lambda-powertools/logger": "^1.12.1",
