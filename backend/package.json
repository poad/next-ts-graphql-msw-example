{
  "name": "@next-ts-cloudfront-ssg-with-backend-template/backend",
  "version": "0.1.0",
  "bin": {
    "backend": "bin/backend.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "format-check": "rome format bin lib lambda",
    "format": "rome format --write bin lib lambda",
    "check": "rome check bin lib lambda",
    "check-fix": "rome check --apply-unsafe bin lib lambda",
    "lint": "pnpm format-check && pnpm check",
    "lint-fix": "pnpm format && pnpm check-fix",
    "clean": "rimraf build",
    "prelint": "pnpm install --no-frozen-lockfile",
    "prelint-fix": "pnpm install --no-frozen-lockfile",
    "prebuild": "pnpm lint",
    "pregenerate": "pnpm install --no-frozen-lockfile",
    "postgenerate": "pnpm build",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "devDependencies": {
    "@apollo/server": "*",
    "@as-integrations/aws-lambda": "*",
    "@aws-lambda-powertools/logger": "*",
    "@graphql-codegen/cli": "*",
    "@graphql-codegen/introspection": "*",
    "@graphql-codegen/jsdoc": "*",
    "@graphql-codegen/typescript": "*",
    "@graphql-codegen/typescript-resolvers": "*",
    "@graphql-eslint/eslint-plugin": "*",
    "@graphql-tools/graphql-file-loader": "*",
    "@graphql-tools/load": "*",
    "@graphql-tools/schema": "*",
    "@swc/cli": "*",
    "@swc/core": "*",
    "@swc/helpers": "*",
    "@types/aws-lambda": "*",
    "@types/node": "*",
    "aws-cdk": "*",
    "esbuild": "*",
    "graphql": "*",
    "graphql-codegen-typescript-validation-schema": "*",
    "log4js": "*",
    "regenerator-runtime": "*",
    "rome": "*",
    "ts-node": "*",
    "typescript": "*",
    "yup": "*"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "*",
    "aws-cdk-lib": "*",
    "aws-lambda": "*",
    "constructs": "*",
    "graphql": "*",
    "source-map-support": "*"
  }
}
