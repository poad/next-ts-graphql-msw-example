{
  "name": "@next-ts-cloudfront-ssg-with-backend-template/frontend",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next build",
    "start": "next start",
    "prettier:fix": "prettier --write src",
    "eslint:fix": "eslint --fix",
    "prettier:check": "prettier --check src",
    "eslint:check": "eslint",
    "lint": "pnpm run --parallel /^.*:check/",
    "lint-fix": "pnpm run /^.*:fix/",
    "clean": "rimraf build",
    "generate": "graphql-codegen --config codegen.yml",
    "analyze": "cross-env ANALYZE=true pnpm build",
    "all": "pnpm lint && pnpm build"
  },
  "dependencies": {
    "@apollo/client": "^3.10.4",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "cross-env": "^7.0.3",
    "graphql": "^16.8.1",
    "next": "^14.2.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.4.5",
    "web-vitals": "^4.1.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.6",
    "@graphql-codegen/typescript-msw": "^3.0.0",
    "@graphql-codegen/typescript-react-apollo": "^4.3.0",
    "@graphql-typed-document-node/apollo-client-3": "^1.0.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@next/bundle-analyzer": "^14.2.4",
    "encoding": "^0.1.13",
    "msw": "^2.3.1",
    "rimraf": "^5.0.7"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "engines": {
    "npm": "use pnpm please!",
    "yarn": "use pnpm please!",
    "pnpm": ">=8.7.4"
  }
}
