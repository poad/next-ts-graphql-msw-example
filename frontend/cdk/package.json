{
  "name": "@next-ts-cloudfront-ssg-with-backend-template/cdk",
  "version": "0.1.0",
  "bin": {
    "cognito": "bin/cloudfront-s3-cdn.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "format-check": "rome format bin lib",
    "format": "rome format --write bin lib",
    "check": "rome check bin lib",
    "check-fix": "rome check --apply-unsafe bin lib",
    "lint": "pnpm format-check && pnpm check",
    "lint-fix": "pnpm format && pnpm check-fix",
    "clean": "rimraf build",
    "prelint": "pnpm install --no-frozen-lockfile",
    "prelint-fix": "pnpm install --no-frozen-lockfile",
    "prebuild": "pnpm lint"
  },
  "devDependencies": {
    "@swc/cli": "latest",
    "@swc/core": "latest",
    "@swc/helpers": "latest",
    "@types/node": "latest",
    "aws-cdk": "latest",
    "regenerator-runtime": "latest",
    "rome": "latest",
    "ts-node": "latest",
    "typescript": "latest"
  },
  "dependencies": {
    "aws-cdk-lib": "latest",
    "constructs": "latest",
    "esbuild": "latest",
    "source-map-support": "latest"
  }
}
